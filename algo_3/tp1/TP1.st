Object subclass: #AlgoFix	instanceVariableNames: 'PlanilladePintores ContadorDeLaEmpresa'	classVariableNames: ''	package: 'TP1'!!AlgoFix methodsFor: 'initialization' stamp: 'joacobetz 5/25/2020 22:13'!initialize 	PlanilladePintores := OrderedCollection new.	ContadorDeLaEmpresa := Contador new.! !!AlgoFix methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 04:24'!crearPintura: unNombre conPrecioPorLitro: unPrecio manosPincel: unaCantidadDeManosPincel manosRodillo: unaCantidadDeManosRodillo	|unaPintura|	unaPintura :=Pintura conNombre:unNombre precio: unPrecio manosPincel: unaCantidadDeManosPincel   manosRodillo: unaCantidadDeManosRodillo.	^unaPintura ! !!AlgoFix methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 04:24'!manoDeObra: unPrecioManoDeObra masMateriales: unPrecioMateriales	^ContadorDeLaEmpresa calcularManoDeObra: unPrecioManoDeObra masMateriales: unPrecioMateriales  ! !!AlgoFix methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 04:24'!registrarPintorDePincel: unNombre  conValorHora: unPrecio	|unaTecnica unPintor|	unaTecnica :=TecnicaPincel new.	unPintor:= Pintor conNombre: unNombre Tecnica: unaTecnica Precio: unPrecio.	PlanilladePintores add: unPintor.! !!AlgoFix methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 04:24'!presupuestoMasBaratoParaPintarMetrosCuadrados: unTamaño conPintura: unaPintura 	|presupuestoMasBarato|	presupuestoMasBarato := ContadorDeLaEmpresa presupuestoMasBaratoParaPintarMetrosCuadrados: unTamaño  conPintura:unaPintura entre: PlanilladePintores.	 ^presupuestoMasBarato.! !!AlgoFix methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 04:24'!registrarPintorDeRodillo: unNombre conValorHora: unPrecio 	|unaTecnica unPintor|	unaTecnica := TecnicaRodillo new.	unPintor := Pintor conNombre: unNombre Tecnica: unaTecnica Precio: unPrecio.	PlanilladePintores add: unPintor.	! !Object subclass: #Contador	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!Contador methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 04:24'!calcularPresupuestoDe: unPintor paraPintar: unTamaño conPintura: unaPintura 	|unPresupuesto unValor|		unValor :=(self calcularManoDeObra:(unPintor valorManoDeObraPor:unTamaño conPintura: unaPintura ) masMateriales: (unaPintura valorMaterialporTamaño: unTamaño porPintor:unPintor)).	unPresupuesto := Presupuesto conResponsable: (unPintor darNombre) yValor: unValor.	^unPresupuesto. ! !!Contador methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 04:24'!calcularManoDeObra: unValorManoDeObra masMateriales: unValorMateriales	^unValorManoDeObra + unValorMateriales ! !!Contador methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 04:24'!presupuestoMasBaratoParaPintarMetrosCuadrados: unTamaño conPintura: unaPintura entre:planilladePintores	|presupuestoMasBarato presupuesto|		(planilladePintores isEmpty)		ifTrue:[FaltadePintoresRegistradosError signal].			presupuestoMasBarato := Presupuesto ficticio.		planilladePintores do: [ :unPintor |presupuesto := self calcularPresupuestoDe: unPintor paraPintar: unTamaño conPintura: unaPintura.((presupuestoMasBarato esMasCaroQue: presupuesto)) 	ifTrue: [presupuestoMasBarato := presupuesto].].^presupuestoMasBarato 	! !Object subclass: #Descuento	instanceVariableNames: 'descuento metrosParaDescuento'	classVariableNames: ''	package: 'TP1'!!Descuento methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 03:29'!aplicarDescuentoA: unMonto Pintando: unaCantidadDemetros	(unaCantidadDemetros > metrosParaDescuento)		ifTrue: [^ self aplicarDescuentoA: unMonto]		ifFalse:[	^ unMonto ].! !!Descuento methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 03:30'!aplicarDescuentoA:  unMonto	^descuento * unMonto  ! !!Descuento methodsFor: 'initialization' stamp: 'joacobetz 6/22/2020 03:22'!descuento: unDescuento	descuento := unDescuento ! !!Descuento methodsFor: 'initialization' stamp: 'joacobetz 6/22/2020 03:31'!metrosParaDescuento: unaCantidadDeMetros	metrosParaDescuento := unaCantidadDeMetros ! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Descuento class	instanceVariableNames: ''!!Descuento class methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 03:56'!conDescuento: unDescuento yUnaCantidadDeMetrosParaAplicarlo: unaCantidadDeMetros 	|descuentoNuevo|		descuentoNuevo  := Descuento new.	descuentoNuevo  descuento: unDescuento.	descuentoNuevo  metrosParaDescuento: unaCantidadDeMetros.	^descuentoNuevo   ! !Error subclass: #FaltadePintoresRegistradosError	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Object subclass: #MisConstantes	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!MisConstantes methodsFor: 'as yet unclassified' stamp: 'joacobetz 5/25/2020 22:29'!gastoRodillo	^5 "litros"! !!MisConstantes methodsFor: 'as yet unclassified' stamp: 'joacobetz 5/25/2020 22:27'!tiempoPincel	^2 "horas"! !!MisConstantes methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 03:12'!descuentoRodillo	^1! !!MisConstantes methodsFor: 'as yet unclassified' stamp: 'joacobetz 5/25/2020 22:28'!gastoPincel	^4 "litros"! !!MisConstantes methodsFor: 'as yet unclassified' stamp: 'joacobetz 5/25/2020 22:17'!pincel	^'Pincel'! !!MisConstantes methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 03:12'!descuentoPincel	^0.5! !!MisConstantes methodsFor: 'as yet unclassified' stamp: 'joacobetz 5/25/2020 22:29'!tiempoRodillo	^1 "hora"! !!MisConstantes methodsFor: 'as yet unclassified' stamp: 'joacobetz 5/25/2020 22:17'!rodillo	^'Rodillo'! !!MisConstantes methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 03:36'!metrosParaDescuentoRodillo	^Float infinity.! !!MisConstantes methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 03:15'!metrosParaDescuentoPincel	^40 ! !Object subclass: #Pintor	instanceVariableNames: 'Nombre Tecnica Precio'	classVariableNames: ''	package: 'TP1'!!Pintor methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 02:11'!tecnica: unaTecnica 	Tecnica := unaTecnica .! !!Pintor methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 02:12'!precio: unPrecio	Precio:= unPrecio ! !!Pintor methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 04:25'!tiempoDeTrabajoPor: unaCantidadDeMetros conPintura: unaPintura 	|tiempodesuTecnica unaCantidadDeManos |	tiempodesuTecnica := (Tecnica tiempoDeLaTecnica).	unaCantidadDeManos := (unaPintura manosParaTecnica: Tecnica).	^ tiempodesuTecnica * unaCantidadDeMetros * unaCantidadDeManos ! !!Pintor methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 04:25'!valorManoDeObraPor: unTamaño conPintura: unaPintura 	|tiempo monto|	tiempo:= self tiempoDeTrabajoPor: unTamaño conPintura: unaPintura .	monto := (Tecnica aplicarDescuentoPorTecnicaA: (tiempo * Precio) porPintar: unTamaño ).	^ monto! !!Pintor methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 04:25'!darTecnica	^Tecnica! !!Pintor methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 04:25'!darNombre	^ Nombre! !!Pintor methodsFor: 'accessing' stamp: 'joacobetz 6/22/2020 02:10'!nombre: unNombre	Nombre:=unNombre.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pintor class	instanceVariableNames: ''!!Pintor class methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 04:26'!conNombre: unNombre Tecnica: unaTecnica Precio: unPrecio 	|unPintor|	unPintor := Pintor new.	unPintor nombre: unNombre.	unPintor	tecnica: unaTecnica.	unPintor precio: unPrecio.	^unPintor. ! !Object subclass: #Pintura	instanceVariableNames: 'Nombre Precio Manos Constantes'	classVariableNames: ''	package: 'TP1'!!Pintura methodsFor: 'initialization' stamp: 'joacobetz 5/25/2020 22:20'!initialize 	Manos:=Dictionary new.	Constantes := MisConstantes new.! !!Pintura methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 02:36'!precio: unPrecio 	Precio:= unPrecio ! !!Pintura methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 04:25'!manosParaTecnica: unaTecnica	|tecnicaUsada|	tecnicaUsada:=	unaTecnica herramientaUsada.	^Manos at: tecnicaUsada. ! !!Pintura methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 04:26'!valorMaterialporTamaño: unTamaño porPintor: unPintor	|litros|	litros:= self litrosNecesariosParaPintar: unTamaño por: unPintor.	^Precio * litros 	! !!Pintura methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 02:35'!manosRodillo: unaCantidadDeManosRodillo 	Manos at:(Constantes rodillo ) put: unaCantidadDeManosRodillo .! !!Pintura methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 02:34'!manosPincel: unaCantidadDeManosPincel 	Manos at:(Constantes pincel) put: unaCantidadDeManosPincel.! !!Pintura methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 04:25'!litrosNecesariosParaPintar: unTamaño por: unPintor 	|tecnicaUsada  cantidadDeManos |	tecnicaUsada  := unPintor darTecnica.	cantidadDeManos := self manosParaTecnica: tecnicaUsada.	^ (tecnicaUsada gastoDePintura)*unTamaño*cantidadDeManos! !!Pintura methodsFor: 'accessing' stamp: 'joacobetz 6/22/2020 02:35'!nombre: unNombre	Nombre := unNombre ! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pintura class	instanceVariableNames: ''!!Pintura class methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 03:56'!conNombre: unNombre precio: unPrecio manosPincel: unaCantidadDeManosPincel manosRodillo: unaCantidadDeManosRodillo 	|unaPintura|	unaPintura := Pintura new.	unaPintura nombre: unNombre. 	unaPintura precio: unPrecio .	unaPintura manosPincel: unaCantidadDeManosPincel .	unaPintura manosRodillo: unaCantidadDeManosRodillo.	^unaPintura ! !Object subclass: #Presupuesto	instanceVariableNames: 'Monto Trabajador'	classVariableNames: ''	package: 'TP1'!!Presupuesto methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 04:17'!responsable: unTrabajador 	Trabajador := unTrabajador ! !!Presupuesto methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 04:16'!monto: unMonto	Monto:=unMonto ! !!Presupuesto methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 04:26'!valor	^Monto! !!Presupuesto methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 04:12'!esMasCaroQue: otroPresupuesto	^(Monto > (otroPresupuesto valor))! !!Presupuesto methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 04:26'!responsable	^Trabajador! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Presupuesto class	instanceVariableNames: ''!!Presupuesto class methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 04:26'!conResponsable: unTrabajador yValor: unMonto	|unPresupuesto|	unPresupuesto := Presupuesto new.	unPresupuesto responsable: unTrabajador.	unPresupuesto monto: unMonto .	^unPresupuesto ! !!Presupuesto class methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/22/2020 04:21'!ficticio	"le asigna a presupuesto una cadena vacia como trabajador y infitito a su valor"	|unPresupuestoFicticio|	unPresupuestoFicticio := Presupuesto new.	unPresupuestoFicticio responsable: ''.	unPresupuestoFicticio monto: Float infinity.	^unPresupuestoFicticio ! !Object subclass: #Tecnica	instanceVariableNames: 'gastoDePintura herramienta tiempoDeLaTecnica descuento'	classVariableNames: ''	package: 'TP1'!!Tecnica methodsFor: 'accessing' stamp: 'joacobetz 6/23/2020 23:36'!tiempoDeLaTecnica 	^ tiempoDeLaTecnica ! !!Tecnica methodsFor: 'accessing' stamp: 'joacobetz 6/23/2020 23:37'!gastoDePintura 	^ gastoDePintura ! !!Tecnica methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/23/2020 23:37'!aplicarDescuentoPorTecnicaA: unMonto porPintar: unTamaño  	^(descuento aplicarDescuentoA: unMonto  Pintando: unTamaño ) ! !!Tecnica methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/23/2020 23:38'!herramientaUsada 	^herramienta ! !!Tecnica methodsFor: 'as yet unclassified' stamp: 'joacobetz 6/23/2020 23:39'!initilize	self subclassResponsibility ! !Tecnica subclass: #TecnicaPincel	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!TecnicaPincel methodsFor: 'initialization' stamp: 'joacobetz 6/22/2020 03:44'!initialize 	|constantes|	constantes := MisConstantes new.	gastoDePintura := constantes gastoPincel .	tiempoDeLaTecnica := constantes tiempoPincel.	herramienta := constantes pincel.	descuento  := Descuento conDescuento: (constantes descuentoPincel ) yUnaCantidadDeMetrosParaAplicarlo: (constantes metrosParaDescuentoPincel).	! !Tecnica subclass: #TecnicaRodillo	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!TecnicaRodillo methodsFor: 'initialization' stamp: 'joacobetz 6/22/2020 03:40'!initialize 	|constantes|	constantes := MisConstantes new.	gastoDePintura := constantes gastoRodillo  .	tiempoDeLaTecnica := constantes tiempoRodillo .	herramienta := constantes rodillo.	descuento  := Descuento conDescuento: (constantes descuentoRodillo ) yUnaCantidadDeMetrosParaAplicarlo: (constantes metrosParaDescuentoRodillo).! !